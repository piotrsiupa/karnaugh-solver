digraph tricky_one
{
    subgraph inputs
    {
        rank=same;
        node [shape=invhouse];
        i0 [label="a"];
        i1 [label="b"];
        i2 [label="c"];
        i3 [label="d"];
    }
    subgraph negated_inputs
    {
        node [shape=diamond];
        ni0 [label="NOT a"];
        i0 -> ni0;
        ni2 [label="NOT c"];
        i2 -> ni2;
    }
    subgraph products
    {
        node [shape=ellipse];
        s0 [label="AND\n[0] = a AND b"];
        i0, i1 -> s0;
        s1 [label="AND\n[1] = a AND b AND d"];
        i3, s0 -> s1;
        s2 [label="AND\n[2] = NOT a AND NOT c"];
        ni0, ni2 -> s2;
    }
    subgraph final_sums
    {
        node [shape=rectangle, style=filled];
        f0 [label="OR\ntricky_0 = (a AND b AND d) OR (NOT a AND NOT c)"];
        s1, s2 -> f0;
        f1 [label="OR\ntricky_1 = (a AND b) OR (NOT a AND NOT c)"];
        s0, s2 -> f1;
    }
}
