digraph very_optimizable
{
	subgraph inputs
	{
		rank=same;
		node [shape=invhouse];
		i0 [label="a"];
		i1 [label="b"];
		i2 [label="c"];
		i3 [label="d"];
	}
	subgraph negated_inputs
	{
		node [shape=diamond];
		ni0 [label="¬a"];
		i0 -> ni0;
		ni1 [label="¬b"];
		i1 -> ni1;
		ni2 [label="¬c"];
		i2 -> ni2;
		ni3 [label="¬d"];
		i3 -> ni3;
	}
	subgraph products
	{
		node [shape=ellipse];
		s1 [label="∧\n[1] = ¬c ∧ d"];
		ni2, i3 -> s1;
		s2 [label="∧\n[2] = ¬a ∧ b"];
		ni0, i1 -> s2;
		s3 [label="∧\n[3] = ¬a ∧ b ∧ ¬c ∧ d"];
		s2, s1 -> s3;
		s4 [label="∧\n[4] = c ∧ ¬d"];
		i2, ni3 -> s4;
		s5 [label="∧\n[5] = ¬a ∧ b ∧ c ∧ ¬d"];
		s2, s4 -> s5;
		s6 [label="∧\n[6] = a ∧ ¬b"];
		i0, ni1 -> s6;
		s7 [label="∧\n[7] = a ∧ ¬b ∧ ¬c ∧ d"];
		s1, s6 -> s7;
		s8 [label="∧\n[8] = a ∧ ¬b ∧ c ∧ ¬d"];
		s4, s6 -> s8;
	}
	subgraph sums
	{
		node [shape=rectangle];
		s10 [label="∨\n[10] = (¬a ∧ b ∧ c ∧ ¬d) ∨ (a ∧ ¬b ∧ ¬c ∧ d)"];
		s5, s7 -> s10;
		s11 [label="∨\n[11] = (¬a ∧ b ∧ ¬c ∧ d) ∨ (¬a ∧ b ∧ c ∧ ¬d) ∨ (a ∧ ¬b ∧ ¬c ∧ d)"];
		s3, s10 -> s11;
	}
	subgraph final_sums
	{
		node [shape=rectangle, style=filled];
		f0 [label="∨\nfirst = (a ∧ ¬b ∧ c ∧ ¬d) ∨ (¬a ∧ b ∧ ¬c ∧ d) ∨ (¬a ∧ b ∧ c ∧ ¬d) ∨ (a ∧ ¬b ∧ ¬c ∧ d)"];
		s8, s11 -> f0;
	}
}
