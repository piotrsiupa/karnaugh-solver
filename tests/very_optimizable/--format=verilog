module very_optimizable (
	input wire a, b, c, d,
	output wire first, second, third, fourth,
);
	
	// Internal signals
	wire [9:0] prods;
	wire [4:0] sums;
	
	// Products
	assign prods[0] = c & !d;
	assign prods[1] = a & !b;
	assign prods[2] = prods[1] & prods[0];
	assign prods[3] = !c & d;
	assign prods[4] = prods[1] & prods[3];
	assign prods[5] = !a & b;
	assign prods[6] = prods[0] & prods[5];
	assign prods[7] = prods[3] & prods[5];
	assign prods[8] = a & b & c & d;
	assign prods[9] = !a & !b & !c & !d;
	
	// Sums
	assign sums[0] = prods[4] | prods[6];
	assign sums[1] = prods[7] | sums[0];
	assign sums[2] = prods[2] | sums[1];
	assign sums[3] = prods[8] | sums[1];
	assign sums[4] = prods[9] | sums[0];
	
	// Results
	assign first = sums[2];
	assign second = sums[3];
	assign third = sums[1];
	assign fourth = sums[4];
	
endmodule
