digraph no_input
{
	subgraph constants
	{
		node [shape=octagon, style=dashed];
		true [label="⊤"];
		false [label="⊥"];
	}
	subgraph function_0
	{
		subgraph sum
		{
			node [shape=rectangle, style=filled];
			f0 [label="always_true"];
			true -> f0;
		}
	}
	subgraph function_1
	{
		subgraph sum
		{
			node [shape=rectangle, style=filled];
			f1 [label="always_false"];
			false -> f1;
		}
	}
	subgraph function_2
	{
		subgraph sum
		{
			node [shape=rectangle, style=filled];
			f2 [label="dont_care"];
			false -> f2;
		}
	}
}
