digraph no_input
{
	subgraph constants
	{
		node [shape=octagon, style=dashed];
		true [label="⊤"];
		false [label="⊥"];
	}
	subgraph final_sums
	{
		node [shape=rectangle, style=filled];
		f0 [label="always_true"];
		true -> f0;
		f1 [label="always_false"];
		false -> f1;
		f2 [label="dont_care"];
		false -> f2;
	}
}
