digraph no_input_names
{
	subgraph inputs
	{
		node [shape=house];
		i0 [label="i0"];
		i1 [label="i1"];
		i2 [label="i2"];
		i3 [label="i3"];
	}
	subgraph products
	{
		node [shape=ellipse];
		edge [taillabel="&"];
		s0 [label="[0]"];
		s0 -> i0, i2, i3;
		s1 [label="[1]"];
		s1 -> i1, i3;
	}
	subgraph final_sums
	{
		node [shape=rectangle, style=filled];
		edge [taillabel="||"];
		f0 [label="f0"];
		f0 -> s0, s1;
	}
}
