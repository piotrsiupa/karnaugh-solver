digraph no_input_names
{
	subgraph inputs
	{
		rank=same;
		node [shape=invhouse];
		i0 [label="i0"];
		i1 [label="i1"];
		i2 [label="i2"];
		i3 [label="i3"];
	}
	subgraph products
	{
		node [shape=ellipse];
		s0 [label="∧\n[0] = i0 ∧ i2 ∧ i3"];
		i0, i2, i3 -> s0;
		s1 [label="∧\n[1] = i1 ∧ i3"];
		i1, i3 -> s1;
	}
	subgraph final_sums
	{
		node [shape=rectangle, style=filled];
		f0 [label="∨\nf0 = (i0 ∧ i2 ∧ i3) ∨ (i1 ∧ i3)"];
		s0, s1 -> f0;
	}
}
