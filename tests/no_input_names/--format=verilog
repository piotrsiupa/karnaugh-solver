module no_input_names (
	input wire [3:0] i,
	output wire [0:0] o,
);
	
	// Internal signals
	wire [1:0] int_prods;
	wire [0:0] int_sums;
	
	// Products
	assign int_prods[0] = i[0] & i[2] & i[3];
	assign int_prods[1] = i[1] & i[3];
	
	// Sums
	assign int_sums[0] = int_prods[0] | int_prods[1];
	
	// Results
	assign o[0] = int_sums[0];
	
endmodule
