digraph without_function_names
{
	subgraph inputs
	{
		rank=same;
		node [shape=invhouse];
		i0 [label="a"];
		i1 [label="b"];
		i2 [label="c"];
		i3 [label="d"];
	}
	subgraph function_0
	{
		subgraph negated_inputs
		{
			node [shape=diamond];
			f0_ni0_0 [label="¬a"];
			i0 -> f0_ni0_0;
		}
		subgraph products
		{
			node [shape=ellipse];
			f0_s0 [label="∧\n[0] = ¬a ∧ d"];
			f0_ni0_0, i3 -> f0_s0;
		}
		subgraph sum
		{
			node [shape=rectangle, style=filled];
			f0 [label="f0 = ¬a ∧ d"];
			f0_s0 -> f0;
		}
	}
	subgraph function_1
	{
		subgraph negated_inputs
		{
			node [shape=diamond];
			f1_ni0_0 [label="¬a"];
			i0 -> f1_ni0_0;
			f1_ni1_0 [label="¬b"];
			i1 -> f1_ni1_0;
		}
		subgraph products
		{
			node [shape=ellipse];
			f1_s0 [label="∧\n[1] = ¬a ∧ ¬b ∧ c"];
			f1_ni0_0, f1_ni1_0, i2 -> f1_s0;
		}
		subgraph sum
		{
			node [shape=rectangle, style=filled];
			f1 [label="f1 = ¬a ∧ ¬b ∧ c"];
			f1_s0 -> f1;
		}
	}
}
