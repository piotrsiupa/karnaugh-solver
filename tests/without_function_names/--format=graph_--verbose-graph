digraph without_function_names
{
	subgraph inputs
	{
		rank=same;
		node [shape=invhouse];
		i0 [label="a"];
		i1 [label="b"];
		i2 [label="c"];
		i3 [label="d"];
	}
	subgraph negated_inputs
	{
		node [shape=diamond];
		ni0 [label="!a"];
		i0 -> ni0;
		ni1 [label="!b"];
		i1 -> ni1;
	}
	subgraph products
	{
		node [shape=ellipse];
		s0 [label="&&\n[0] = !a && !b"];
		ni0, ni1 -> s0;
		s1 [label="&&\n[1] = !a && !b && c"];
		i2, s0 -> s1;
	}
	subgraph final_sums
	{
		node [shape=rectangle, style=filled];
		f0 [label="f0 = !a && !b"];
		s0 -> f0;
		f1 [label="f1 = !a && !b && c"];
		s1 -> f1;
	}
}
