digraph hard_to_print
{
	subgraph inputs
	{
		rank=same;
		node [shape=invhouse];
		i0 [label="q"];
		i1 [label="x"];
		i2 [label="y"];
		i3 [label="z"];
	}
	subgraph constants
	{
		node [shape=octagon, style=dashed];
		true [label="⊤"];
	}
	subgraph negated_inputs
	{
		node [shape=diamond];
		ni0 [label="¬q"];
		i0 -> ni0;
		ni1 [label="¬x"];
		i1 -> ni1;
		ni2 [label="¬y"];
		i2 -> ni2;
	}
	subgraph products
	{
		node [shape=ellipse];
		s0 [label="∧\n[0] = ¬q ∧ ¬x"];
		ni0, ni1 -> s0;
		s1 [label="∧\n[1] = ¬q ∧ ¬x ∧ ¬y"];
		ni2, s0 -> s1;
	}
	subgraph final_sums
	{
		node [shape=rectangle, style=filled];
		f1 [label="all = ⊤"];
		true -> f1;
		f3 [label="even_less = ¬q ∧ ¬x ∧ ¬y"];
		s1 -> f3;
		f6 [label="all_again = ⊤"];
		true -> f6;
	}
}
