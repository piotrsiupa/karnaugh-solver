none(q, x, y, z) = FALSE
all(q, x, y, z) = TRUE
some(q, x, y, z) = NOT q AND NOT x
even_less(q, x, y, z) = NOT q AND NOT x AND NOT y
more_than_less(q, x, y, z) = NOT z OR (NOT q AND NOT x AND NOT y)
none_again(q, x, y, z) = FALSE
all_again(q, x, y, z) = TRUE
some_again(q, x, y, z) = NOT q AND NOT x
more_than_less_again(q, x, y, z) = NOT z OR (NOT q AND NOT x AND NOT y)
half(q, x, y, z) = NOT z
some_the_third_time(q, x, y, z) = NOT q AND NOT x
more_than_less_the_third_time(q, x, y, z) = NOT z OR (NOT q AND NOT x AND NOT y)
most(q, x, y, z) = q OR y
even_more(q, x, y, z) = q OR NOT x OR y
half_again(q, x, y, z) = NOT z
