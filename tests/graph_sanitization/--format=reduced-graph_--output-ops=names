digraph graph_sanitization
{
	subgraph products
	{
		node [shape=ellipse];
		s0 [label="p/\"/\"::\\\\n->; = a AND c", style=filled];
		s1 [label="[1] = NOT a AND c"];
		s2 [label="[2] = a AND NOT b AND NOT c"];
	}
	subgraph sums
	{
		node [shape=rectangle];
		s3 [label="OR\na!@\\$%^&*()m", style=filled];
		s1, s2 -> s3;
	}
}
