digraph graph_sanitization
{
	subgraph products
	{
		node [shape=ellipse];
		s0 [label="p/\"/\"::\\\\n->; = a /\\ c", style=filled];
		s1 [label="[1] = ~a /\\ c"];
		s2 [label="[2] = a /\\ ~b /\\ ~c"];
	}
	subgraph sums
	{
		node [shape=rectangle];
		s3 [label="\\/\na!@\\$%^&*()m", style=filled];
		s1, s2 -> s3;
	}
}
