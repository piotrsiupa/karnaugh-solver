digraph graph_sanitization
{
	subgraph inputs
	{
		rank=same;
		node [shape=invhouse];
		i0 [label="a"];
		i1 [label="b"];
		i2 [label="c"];
	}
	subgraph negated_inputs
	{
		node [shape=diamond];
		ni0 [label="NOT a"];
		i0 -> ni0;
		ni1 [label="NOT b"];
		i1 -> ni1;
		ni2 [label="NOT c"];
		i2 -> ni2;
	}
	subgraph products
	{
		node [shape=ellipse];
		s0 [label="AND\n[0] = a AND c"];
		i0, i2 -> s0;
		s1 [label="AND\n[1] = NOT a AND c"];
		ni0, i2 -> s1;
		s2 [label="AND\n[2] = a AND NOT b AND NOT c"];
		i0, ni1, ni2 -> s2;
	}
	subgraph final_sums
	{
		node [shape=rectangle, style=filled];
		f0 [label="OR\na!@\\$%^&*()m = (NOT a AND c) OR (a AND NOT b AND NOT c)"];
		s1, s2 -> f0;
		f1 [label="p/\"/\"::\\\\n->; = a AND c"];
		s0 -> f1;
	}
}
